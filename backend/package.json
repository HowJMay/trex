{
  "name": "yttrex-backend",
  "version": "1.3.2",
  "description": "youtube.tracking.exposed - backend data collector",
  "scripts": {
    "watch": "node_modules/.bin/nodemon --config config/nodemon.json bin/server --elastic=disabled --key=fuffa --DEBUG=lib:*,yttrex",
    "start": "DEBUG=*,-body-parser:*,-express:*,-lib:mongo node bin/server --elastic=disabled",
    "parserv": "DEBUG=* node bin/parserv2 --elastic=disabled",
    "t:lib": "mocha tests/lib/*"
  },
  "author": "Claudio Agosti <claudio@tracking.exposed> and https://github.com/tracking-exposed/yttrex/graphs/contributors",
  "license": "AGPL-3.0",
  "dependencies": {
    "bluebird": "^3.5.2",
    "body-parser": "^1.15.2",
    "bs58": "^3.1.0",
    "cookie": "^0.3.1",
    "cors": "^2.8.1",
    "debug": "^2.2.0",
    "elasticsearch": "^15.4.1",
    "express": "^4.14.0",
    "food-words": "^1.1.0",
    "moment": "^2.14.1",
    "mongodb2": "npm:mongodb@^2.1.8",
    "mongodb3": "npm:mongodb@3.2.3",
    "nacl-signature": "^1.0.0",
    "nconf": "^0.8.4",
    "nodemon": "^2.0.2",
    "numeral": "^2.0.6",
    "request": "^2.75.0",
    "tweetnacl": "^0.14.3"
  },
  "devDependencies": {
    "jsdom": "^13.0.0",
    "chai": "^4.2.0",
    "mocha": "^5.2.0"
  }
}
